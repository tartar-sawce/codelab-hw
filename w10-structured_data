import csv
from typing import Counter

with open('marriage_data_india.csv', mode='r', newline='', encoding="utf-8") as csvfile:
    shadishuda = csv.DictReader(csvfile)
    love_count = Counter()
    for shadi in shadishuda:
        p = shadi["Marriage_Type"]
        love_count[p] += 1

    print(love_count)

